[user]
    name = Marc Hibbins
    email = marchibbins@gmail.com
[core]
    editor = /usr/bin/vim
    excludesfile = ~/.gitignore_global
[color]
    ui = true
[color "branch"]
    current = yellow reverse
    local = yellow
    remote = green
[color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold
[color "status"]
    added = yellow
    changed = green
    untracked = cyan
[alias]
    co = checkout
    cm = commit -m
    cam = commit -am
    s = status
    ss = status -s
    df = diff
    lg = log
